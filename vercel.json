{
  "rewrites": [
    {
      "source": "/((?!glb_asset).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/glb_asset/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

